require "optparse"
require "colored"

begin
	OptionParser.new do |parser|

		parser.on("-v", "--version") do |v|
			puts "Tekrand 1.0\nRandom number generator from Tekpack\nReleased under Apache 2.0\nTekronet 2021 <tekronet.github.io>"
		end
		
	end.parse!
end
